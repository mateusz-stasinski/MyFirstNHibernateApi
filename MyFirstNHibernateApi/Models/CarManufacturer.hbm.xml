<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="NHibernateExample.Models" assembly="NHibernateExample">
CarManufacturer
	<class name="CarManufacturer" table="CarManufacturer">

		<id name="Id">
			<column name="Id" sql-type="int" not-null="true"/>
			<generator class="native" />
		</id>

		<property name="Name">
			<column name="Name" length="16" not-null="true" />
		</property>
	</class>

</hibernate-mapping>